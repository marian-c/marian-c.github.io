{
    "$schema": "vscode://schemas/color-theme",
    "name": "Retro Assembler Blue",
    "type": "dark",
    "colors": {
    "foreground": "#ffffff",
    "panel.background": "#242424",
    "panel.border": "#606060",

    "editor.background": "#0c4368",
    "editor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#496585",
    "editor.selectionBackground": "#387ead",
    "editor.selectionHighlightBackground": "#4a75a7",
    "editor.findMatchHighlightBackground": "#5b7ea7",
    "editor.findRangeHighlightBackground": "#5b7ea7",

    "editorCursor.foreground": "#e8e8e8",
    "editorWhitespace.foreground": "#1d5e8a",
    "editorLineNumber.foreground": "#6cb1df",
    "editorLineNumber.activeForeground": "#f0f0f0",

    "sideBar.background": "#07263b",
    "sideBar.foreground": "#f0f0f0",

    "titleBar.activeBackground": "#181818",
    "activityBar.background": "#083452",
    "dropdown.background": "#242424",
    "statusBar.background": "#181818",
    "statusBar.noFolderBackground": "#181818",
    "statusBar.debuggingBackground": "#181818",

    "editorGroupHeader.tabsBackground": "#07263b",
    "editorGroup.dropBackground": "#07263b",
    "tab.activeBackground": "#0c4368",
    "tab.inactiveBackground": "#093450",
    "tab.hoverBackground": "#12537e",

    "input.background": "#f5f5f5",
    "input.foreground": "#303030",
    "input.placeholderForeground": "#808080",
    "input.border": "#181818",

    "list.activeSelectionBackground": "#3360a3",
    "list.hoverBackground": "#395c91",
    "list.inactiveSelectionBackground": "#3e4e66",

    "editorWidget.background": "#404040",
    "editorWidget.resizeBorder": "#303030"

    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#c8c8c8",
                "fontStyle": ""
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#d1f1a9"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#97d6f3",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.directive",
            "settings": {
                "foreground": "#6ba3ff",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.instruction",
            "settings": {
                "foreground": "#69cbf8"
            }
        },
        {
            "scope": "keyword.instructionother",
            "settings": {
                "foreground": "#e293e2"
            }
        },
        {
            "scope": "keyword.register",
            "settings": {
                "foreground": "#1ecf44"
            }
        },
        {
            "scope": "keyword.locallabel",
            "settings": {
                "foreground": "#ffbf8b"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#b0c1fa"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#ffe986"
            }
        },

        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "header",
            "settings": {
                "foreground": "#000080"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#646695"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "entity.name.tag.css",
            "settings": {
                "foreground": "#d7ba7d"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#9cdcfe"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.class.mixin.css",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.attribute.scss",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#d7ba7d"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#f44747"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "beginning.punctuation.definition.quote.markdown",
            "settings": {
                "foreground": "#608b4e"
            }
        },
        {
            "scope": "beginning.punctuation.definition.list.markdown",
            "settings": {
                "foreground": "#6796e6"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "scope": "meta.selector",
            "settings": {
                "foreground": "#d7ba7d"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "scope": "meta.preprocessor",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "scope": "meta.structure.dictionary.key.python",
            "settings": {
                "foreground": "#9cdcfe"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#d16969"
            }
        },
        {
            "name": "String interpolation",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#d4d4d4"
            }
        },
        {
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "variable.css",
                "variable.scss",
                "variable.other.less",
                "source.coffee.embedded"
            ],
            "settings": {
                "foreground": "#9cdcfe"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.instanceof",
                "keyword.operator.logical.python"
            ],
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "support.function.git-rebase",
            "settings": {
                "foreground": "#9cdcfe"
            }
        },
        {
            "scope": "constant.sha.git-rebase",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": [
                "storage.modifier.import.java",
                "variable.language.wildcard.java",
                "storage.modifier.package.java"
            ],
            "settings": {
                "foreground": "#d4d4d4"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796e6"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#cd9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#f44747"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#b267e6"
            }
        }
    ]
}